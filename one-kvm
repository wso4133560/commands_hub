# 带有和核显声音的远程
docker run -e HWENCODER=vaapi --name kvmd -itd \
    --device /dev/video0:/dev/video0 \
    --device /dev/ttyUSB0:/dev/ttyUSB0 \
    --device /dev/snd:/dev/snd \
    --device /dev/dri:/dev/dri \
    -p 4430:4430 -p 5900:5900 -p 623:623 \
    silentwind0/kvmd

one-kvm.mofeng.run/start_install/docker_install
