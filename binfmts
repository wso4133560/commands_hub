# 显示binfmt的状态
update-binfmts --display

# update-binfmts 安装x86-64的elf文件
sudo update-binfmts  --install FEXLoader /llvm-fex-jit/FEX/build/Bin/FEXLoader.sh --magic '\x7f\x45\x4c\x46\x02\x01\x01\x00\x00\x00\x00\x00\x00\x00\x00\x00\x02\x00\x3e\x00' --mask '\xff\xff\xff\xff\xff\xfe\xfe\xfc\xff\xff\xff\xff\xff\xff\xff\xff\xfe\xff\xff\xff'

