# 命令修改端口映射
vim /etc/config/firewall
config redirect                                 
        option dest 'lan'                       
        option target 'DNAT'                    
        option name 'ssh'                       
        option src 'wan'                        
        option src_dport '33333'                
        option dest_ip '192.168.100.1'          
        option dest_port '80'

/etc/init.d/firewall reload


# 添加tun接口
vim /etc/config/network
config interface 'tun'
    option ifname 'tun0'
    option proto 'none'
    option auto '1'

/etc/init.d/network restart

ip link set tun0 up
